{
  "name": "ahj-testing",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "webpack serve",
    "build": "webpack --mode production",
    "test": "jest --selectProjects unit",
    "test:coverage": "jest --selectProjects unit --coverage",
    "e2e": "jest --selectProjects e2e --runInBand",
    "serve:dist": "http-server ./dist -p 8080",
    "e2e:ci": "wait-on http://localhost:8080 && npm run e2e",
    "test:e2e:full": "npm run build && concurrently -k -s first \"npm run serve:dist\" \"npm run e2e:ci\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.28.3",
    "@babel/core": "^7.28.4",
    "@babel/preset-env": "^7.23.0",
    "babel-jest": "^30.2.0",
    "babel-loader": "^9.1.3",
    "concurrently": "^9.2.1",
    "copy-webpack-plugin": "^13.0.1",
    "css-loader": "^6.0.0",
    "html-webpack-plugin": "^5.0.0",
    "http-server": "^14.1.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.2.0",
    "jest-puppeteer": "^11.0.0",
    "mini-css-extract-plugin": "^2.7.6",
    "puppeteer": "^24.25.0",
    "style-loader": "^3.0.0",
    "wait-on": "^7.2.0",
    "webpack": "^5.0.0",
    "webpack-cli": "^5.0.0",
    "webpack-dev-server": "^4.0.0"
  },
  "dependencies": {
    "core-js": "^3.46.0"
  }
}
